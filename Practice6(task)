#include <iostream>


using namespace std;



int main() {
    int n;
    cin >> n;
    int sum = 0;
    while (n!=0){
        int number = n %10;
        if (number > 2 && number % 2!=0){
            sum += number;
        }
        n /= 10;
        }
        cout << "Result is: " << sum;
        return 0;
}
